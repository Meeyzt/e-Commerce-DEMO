
@model List<e_commerce_DEMO.Models.Product.Product>

<h1>@ViewData["Heading"]</h1>
<table border="1">
    <tr>
        <td>ID</td>
        <td>URUN AD</td>
        <td>FIYAT</td>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.ID</td>
            <td>@p.Name</td>
            <td>@p.Price</td>
        </tr>
    }
</table>


